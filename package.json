        "gulp-imagemin": "^8.0.0",
        "gulp-rename": "^2.0.0",
        "gulp-replace": "^1.1.4",
        "gulp-sass": "^5.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "sass": "^1.62.1"
    },
    "devDependencies": {
        "del": "^6.1.1",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
